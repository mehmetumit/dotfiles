set page-padding 2
map n navigate next
map N navigate previous
map K zoom in
map J zoom out
map i recolor
map p print
map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map <C-b> toggle_statusbar
map  B focus_inputbar ":bmark "
map  b focus_inputbar ":blist "
map  x focus_inputbar ":bdelete "
